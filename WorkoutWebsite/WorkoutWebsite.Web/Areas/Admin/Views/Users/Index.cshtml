@model IEnumerable<AdminUserInfoModel>

@{
    ViewData["Title"] = "User Administration";
}

<h2>@ViewData["Title"]</h2>

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>UserName</th>
            <th>Email</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th>@item.UserName</th>
                <th>@item.Email</th>
                <th></th>
            </tr>
        }
    </tbody>
</table>


